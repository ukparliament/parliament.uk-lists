- title("#{@party.name} - #{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')} - #{@parliament.date_range(date_format: '%Y')} #{t('parliament')} #{t('parliaments.a_to_z')}")
#content.section--primary{ tabindex: "0" }
  .container
    %h1
      %span= "#{@party.name} - #{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')}"
      = link_to("#{@parliament.date_range(date_format: '%Y')} #{t('parliament').capitalize}", parliament_path(@parliament.graph_id))
      %span= "#{t('parliaments.a_to_z')} - #{t('a_to_z.select_letter')}"

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_house_party_members_a_z_letter_path, route_args: [@parliament.graph_id, @house.graph_id, @party.graph_id], all_path: @all_path }
