- title("#{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')} #{@parliament.date_range(date_format: '%Y')} #{t('parliament')} #{t('parliaments.a_to_z')} #{t('parliaments.showing_results')} '#{params[:letter].capitalize}'")
.section--primary
  .container
    %h1
      %small= link_to("#{@parliament.date_range(date_format: '%Y')} #{t('parliament').capitalize}", parliament_path(@parliament.graph_id))
      %span= "#{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')}"
      %span.context= "#{t('parliaments.a_to_z')} - #{t('parliaments.showing_results')} #{params[:letter].capitalize}"

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_house_members_a_z_letter_path, route_args: [@parliament.graph_id, @house.graph_id], all_path: @all_path }

%section#content{ tabindex: "0" }
  .container
    = render partial: "pugin/elements/list", locals: { item_type: "people", data: @people }
