- title("#{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')} - #{@parliament.date_range(date_format: '%Y')} #{t('parliament')}")
.section--primary
  .container
    %h1
      %span= "#{Parliament::Utils::Helpers::HousesHelper.commons?(@house) ? t('mp_plural') : t('lord_plural')}"
      = link_to("#{@parliament.date_range(date_format: '%Y')} #{t('parliament')}", parliament_path(@parliament.graph_id))
      %span= "#{t('a_to_z.a_to_z')} - #{t('a_to_z.showing_all')}"

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_house_members_a_z_letter_path, route_args: [@parliament.graph_id, @house.graph_id] }

%section#content{ tabindex: "0" }
  .container
    = render partial: "pugin/elements/list", locals: { item_type: "people", data: @people }
