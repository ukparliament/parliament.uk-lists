- title("#{@party.name} - #{t('mps_and_lords')} - #{@parliament.date_range(date_format: '%Y')} #{t('parliament')}")
.section--primary
  .container
    %h1
      %span= "#{@party.name} - #{t('mps_and_lords')}"
      = link_to "#{@parliament.date_range(date_format: '%Y')} #{t('parliament')}", parliament_path(@parliament.graph_id)
      %span= "#{t('a_to_z.a_to_z')} - #{t('a_to_z.showing_all')}"

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_party_members_a_z_letter_path, route_args: [@parliament.graph_id, @party.graph_id]}

%section#content
  .container
    = render partial: "pugin/elements/list", locals: {item_type: "people", data: @people}
